<%= content_for :rel_tag do %>
	<meta property="og:title" content= "<%= title %>" />
	<meta property="og:type" content="article" />
	<meta property="og:image" content="<%= @feed.media_image ? @feed.media_image.gsub(/\?(.*)/,'') : '' %>" />
	<meta property="og:url" content="http://gorss.jbmyid.in<%= feed_path(@feed) %>" />
	<meta property="og:description" content="<%= Sanitize.clean @feed.data.description %>" />
	<link rel="image_src" href="<%= @feed.media_image ? @feed.media_image.gsub(/\?(.*)/,'') : '' %>" />
<% end %>
<%= render 'user/shared/full_feed', feed: @feed %>
<script type="text/javascript">
	$(function(){
		$("#fullFeedPopup").animate({left: "0"})
		$(".back").click(function(){
			window.location = "<%= root_path %>"
		})
	})
</script>