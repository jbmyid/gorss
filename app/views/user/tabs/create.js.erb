<% if @tab.errors.any? %>
  alert("<%= @tab.errors.full_messages.join(',')%>")
<% else %>
  <% if params[:import]%>
  	window.close();
  <% else %>
	  $("#category_<%= @tab.category_id %> input").val("")
	  html = "<%= j(render @tab) %>";
	  $("#category_ul_<%= @tab.category_id %>").append(html);
	  apply_dragable_tabs()
	  $('#feed-container').masonry('reload');
  <% end %>
<% end %>

