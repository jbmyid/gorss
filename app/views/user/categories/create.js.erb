<% if @category.errors.any? %>
  alert("<%= @category.errors.full_messages.join(',')%>")
<% else %>
  $(".new_category_form input").val("")
  html = "<%= j(render @category).html_safe %>";
  $(".categories_container").append(html);
  $('#feed-container').masonry('reload');
  apply_dragable_tabs()
  apply_color_picker()
<% end %>

